{"remainingRequest":"D:\\xiaomi\\vue\\node_modules\\babel-loader\\lib\\index.js!D:\\xiaomi\\vue\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!D:\\xiaomi\\vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\xiaomi\\vue\\src\\views\\ShopCart.vue?vue&type=script&lang=js&","dependencies":[{"path":"D:\\xiaomi\\vue\\src\\views\\ShopCart.vue","mtime":1684853306358},{"path":"D:\\xiaomi\\vue\\babel.config.js","mtime":1684853305416},{"path":"D:\\xiaomi\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\xiaomi\\vue\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"D:\\xiaomi\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\xiaomi\\vue\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0GA;AACA;AACA;AACA;AACA;AACAA,kBADA;AAEAC;AAAAC;AAAA,GAFA;AAGAC,MAHA,kBAGA;AACA;AACAC,gBADA;AAEAC,kBAFA;AAGAC,gBAHA;AAIAC;AAJA;AAMA,GAVA;AAWAC,SAXA,qBAWA;AACA;AACAC;AACA;AACA,GAfA;AAgBAC;AACAC,QADA,kBACA;AAAA;;AACAC;AAAAC;AACAJ;AADA;AAAA,SACAK,IADA,CAEA;AACA,qCACA,mCADA,KAEA;AACA;AACA;AACA,OARA;AAUA,KAZA;AAaAC,yBAbA,iCAaAC,GAbA,EAaA;AACA;AACA,KAfA;AAgBAC,aAhBA,qBAgBAC,OAhBA,EAgBAC,GAhBA,EAgBAC,KAhBA,EAgBA;AAAA;;AACAR;AACAC;AACAJ;AADA;AADA,SAGAK,IAHA,CAIA;AACA,qCACA,oCADA,KAEA,CACA;AACA,OATA;AAWA,KA5BA;AA6BAO,WA7BA,mBA6BAC,CA7BA,EA6BA;AACA;AACA;;AACA;AACAC;AACA;;AACAC;AACAA;AACAA;AACAA;AACAA;AACA;AACA,KAzCA;AA0CAC,aA1CA,qBA0CAC,EA1CA,EA0CA;AAAA;;AACAd;AACAC;AAAAJ;AAAA;AADA,SAEAK,IAFA,CAGA;AAAA;AAAA;AAAA,eACA;AAAA,OAJA;AAMA,KAjDA;AAkDAa,eAlDA,yBAkDA;AAAA;;AACA;AAAA;AAAA;AACAf;AAAAC;AAAAJ;AAAA;AAAA,cACA;AAAA;AAAA;AAAA,eACA;AAAA,OAFA;AAIA,KAxDA;AAyDAmB,UAzDA,oBAyDA;AACA;AAAA;AAAA;AACA,2DACA;AACA;AA7DA;AAhBA","names":["name","components","Navi","data","userId","cartlist","search","multipleSelect","created","token","methods","load","axios","headers","then","handleSelectionChange","val","numChange","current","old","index","summary","p","temp","sums","deleteOne","id","batchDelete","submit"],"sourceRoot":"src/views","sources":["ShopCart.vue"],"sourcesContent":["<template xmlns=\"http://www.w3.org/1999/html\">\n<div>\n  <Navi/>\n  <el-table\n      v-loading=\"loading\"\n      element-loading-text=\"拼命加载中\"\n      element-loading-spinner=\"el-icon-loading\"\n      element-loading-background=\"rgba(0, 0, 0, 0.8)\"\n      ref=\"multipleTable\"\n      :data=\"cartlist.filter(data => !search || data.productName.toLowerCase().includes(search.toLowerCase()))\"\n      tooltip-effect=\"dark\"\n      style=\"width: 90%;margin:0 auto\"\n      max-height=\"420\" show-summary :summary-method=\"summary\"\n      border @selection-change=\"handleSelectionChange\"\n      stripe >\n    <el-table-column\n        type=\"selection\"\n        width=\"60\">\n    </el-table-column>\n\n      <el-table-column\n          label=\"图片\"\n          width=\"120\">\n        <template slot-scope=\"scope\">\n          <router-link :to=\"{name:'Product',query:{pid:scope.row.productId}}\">\n            <el-image :src=\"'http://localhost:9090/img/'+scope.row.url\" fit=\"fill\" ></el-image>\n          </router-link>\n        </template>\n      </el-table-column>\n      <el-table-column\n          label=\"商品名\"\n          width=\"120\" >\n        <template slot-scope=\"scope\">\n            <el-link :href=\"'/product/?pid='+scope.row.productId\"\n            :underline=\"false\">{{scope.row.productName}}</el-link>\n        </template>\n      </el-table-column>\n      <el-table-column\n          prop=\"skuName\"\n          label=\"套餐\"\n          width=\"120\">\n      </el-table-column>\n      <el-table-column\n        prop=\"skuProps\"\n        label=\"套餐属性\" ></el-table-column>\n    <el-table-column\n      prop=\"productPrice\"\n      label=\"单价\"\n    width=\"60\">\n      <template slot-scope=\"scope\">\n        <em style=\"font-size: 10px;text-decoration: line-through\">¥{{scope.row.originalPrice}}</em>\n        <br><em style=\"font-size:16px;font-weight: 700; \">¥{{scope.row.sellPrice}}</em>\n\n      </template>\n    </el-table-column>\n    <el-table-column\n      label=\"数量\" width=\"160\">\n      <template slot-scope=\"scope\">\n        <el-input-number  :min=\"1\" size=\"mini\" :max=\"scope.row.stock\" v-model=\"scope.row.cartNum\"\n          @change=\"(current,old)=>numChange(current,old,scope.row.cartId)\"></el-input-number>\n      </template>\n    </el-table-column>\n    <el-table-column label=\"金额\" width=\"100\">\n      <template slot-scope=\"scope\">\n        <em style=\"font-size:16px;font-weight: 700; color: #c81623\">¥{{scope.row.sellPrice*scope.row.cartNum}}</em>\n      </template>\n    </el-table-column>\n    <el-table-column\n      label=\"添加时间\"\n      prop=\"cartTime\" sortable></el-table-column>\n    <el-table-column\n        align=\"right\" fixed=\"right\" width=\"100\">\n      <template slot=\"header\" slot-scope=\"scope\">\n        <el-input\n            v-model=\"search\"\n            size=\"mini\"\n            placeholder=\"搜索商品名\"/>\n      </template>\n      <template slot-scope=\"scope\">\n          <el-button\n              size=\"mini\" @click=\"deleteOne(scope.row.cartId)\"\n              type=\"text\" icon=\"el-icon-delete\">删除</el-button>\n      </template>\n    </el-table-column>\n  </el-table>\n  <el-row type=\"flex\" style=\"width: 90%;margin: 0 auto\" justify=\"space-between\" >\n    <el-col :span=\"6\" >\n      <el-popconfirm\n          confirm-button-text='好的'\n          cancel-button-text='不用了'\n          icon=\"el-icon-info\"\n          icon-color=\"red\"  @confirm=\"batchDelete\"\n          title=\"这是一段内容确定删除吗？\"\n      >\n        <el-button slot=\"reference\" icon=\"el-icon-delete\" type=\"danger\">批量删除</el-button>\n      </el-popconfirm>\n    </el-col>\n    <el-col :span=\"6\"> </el-col>\n    <el-col :span=\"6\" >\n      <el-button style=\"background-color: #ff5000;color: white\" @click=\"submit\">结算</el-button>\n    </el-col>\n  </el-row>\n</div>\n</template>\n\n<script>\nimport {baseURL} from \"../../public/urlConfig\";\nimport axios from \"axios\";\nimport Navi from \"../components/Navi\";\nlet token\nexport default {\n  name: \"ShopCart\",\n  components: {Navi},\n  data(){\n    return{\n      userId:\"\",\n      cartlist:[],\n      search:'',\n      multipleSelect:[],\n    }\n  },\n  created() {\n    this.userId=this.$route.query.uid;\n    token=localStorage.getItem(\"token\")\n    this.load();\n  },\n  methods:{\n    load(){\n      axios.get(baseURL+\"/shopping-cart/list/\"+this.userId,{headers:{\n          token:token}}).then(\n          res=>{\n            if(res.data.code===10001)\n              this.$message.error(res.data.msg)\n            else {\n              this.cartlist = res.data\n            }\n          }\n      )\n    },\n    handleSelectionChange(val) {\n      this.multipleSelect = val\n    },\n    numChange(current,old,index){\n      axios.get(baseURL+\"/shopping-cart/update/\"+index+\"/\"+current,{\n        headers:{\n          token:token}\n      }).then(\n          res=>{\n            if(res.data.code===10001)\n              this.$message.error(res.data.msg)\n            else {\n            }\n          }\n      )\n    },\n    summary(p){\n      const sums = []\n      let temp=0\n      for (let i=0;i<this.multipleSelect.length;i++){\n        temp=temp+this.multipleSelect[i].sellPrice*Number(this.multipleSelect[i].cartNum)\n      }\n      sums[7]=temp\n      sums[1]=this.multipleSelect.length\n      sums[1]+='件商品'\n      sums[7]+='元'\n      sums[0]='总计'\n      return sums\n    },\n    deleteOne(id){\n      axios.delete(baseURL+\"/shopping-cart/\"+id,{\n        headers:{token:token}\n      }).then(\n          res=>{if(res.data===true){this.$message.success(\"删除成功\"),this.load()}\n          else this.$message.error(\"删除失败\")}\n      )\n    },\n    batchDelete(){\n      let ids = this.multipleSelect.map(v => v.cartId)\n      axios.post(baseURL+\"/shopping-cart/ids\",ids,{headers:{token:token}}).then(\n          res=>{if(res.data.code===10000){this.$message.success(\"删除成功\"),this.load()}\n            else this.$message.error(res.data.msg)}\n      )\n    },\n    submit(){\n      let cids=this.multipleSelect.map(v=>v.cartId)\n      if(cids==\"\")this.$message.error(\"至少选择一件商品\")\n      else this.$router.push(\"/order?cids=\"+cids)\n    }\n  }\n}\n</script>\n\n<style scoped>\n</style>"]}]}